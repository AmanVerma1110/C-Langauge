#include<stdio.h> 
#include<stdlib.h>
#include<math.h>
int main() 
{
   int num , pow ;
   int result = 1; 
    
    printf("Enter the num :: ");
    scanf("%d",&num);
    
    printf("Enter the power :: ");
    scanf("%d",&pow);
    
    while(pow>0)
    {
		result = result * num;	
        pow--;	
	}
    
    printf("result is :: %d \n ",result);
    return 0 ;
}
