//To Find whether the given integer is divisible by 7
// Solution 

#include<stdio.h>
#include<stdlib.h>
 
int main()
{
    int i;
    printf("Enter the number :\t");
    scanf("%d",&i);
    if (i%7 == 0)
    printf("Number is divisible by 7");
    else
    printf("Number is not divisible by 7");
    
    return 0;
}
