/* Given 10 Excercices to practice format specifiers using the "printf" function. 
write down what will be printed at each of them then make sure of by testing result in code :: Blocks.  */

#include<stdio.h>

int main() 
{

printf("We have %d coin in the bank\n",100); // \n use for new line & %d for integer number  
printf("We have %f coins in the bank\n",125.7); // %f use for floting numbers
printf("Year = %d\n",2022);
printf("Your average grade is : %.1f\n",95.5); // .1 use for remove zero from floting value 
printf("num1 = %d, num2 = %d, sum = %d \n",5,7,5+7);
printf("num1 = %f, num2 = %f, sum = %f \n", 5.2, 7.3, 5.2+7.3);
printf("num1 = %d , num2 = %d, sub = %d \n",5,3,5-3);
printf("a = %d , b = %d , sum=a+b= %d\n",3,5,3+5);
printf("a=%d,b=%d,sum=%d+%d=%d\n",3,5,3,5,3+5);
printf("a=%d, b=%d, sum=a+b=%d+%d=%d\n",3,5,3,5,3+5);


return 0;
 }
 
 // #2
 
 #include<stdio.h>

int main()
{
	
	printf("%d+%d=%d \n",5,2,5+2);
	printf("%d-%d=%d \n",5,2,5-2);
	printf("%d/%d=%d \n",5,2,5/2);
	printf("%d*%d=%d \n",5,2,5*2);
	
	return 0;
}
 
